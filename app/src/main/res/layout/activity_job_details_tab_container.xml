<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="jobDetailsTabContainerVM" type="com.marveloustandur.app.modules.jobdetailstabcontainer.data.viewmodel.JobDetailsTabContainerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowbackOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowbackOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group162799"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group162799"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtJobdetails"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{jobDetailsTabContainerVM.jobDetailsTabContainerModel.txtJobdetails}"
                style="@style/txtPlusJakartaSansbold18" tools:text="@string/lbl_job_details"/>
            <ImageView android:id="@+id/imageBookmarkOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_bookmark"
                tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark"/>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameStackcardanoone"
            android:layout_height="@dimen/_688pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnwallet"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearColumncardanoon"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_24pxv"
                    android:paddingEnd="@dimen/_72pxh"
                    android:paddingStart="@dimen/_72pxh"
                    android:paddingTop="@dimen/_24pxv" style="@style/groupStyle{&quot;name&quot;:&quot;onPrimaryContainer&quot;,&quot;opacity&quot;:1}cornerRadius">
                    <FrameLayout android:id="@+id/frameLogo"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_79pxv"
                        android:layout_width="@dimen/_79pxh"
                        android:paddingHorizontal="@dimen/_19pxh"
                        android:paddingVertical="@dimen/_19pxv" style="@style/groupStylenamegray_1">
                        <ImageView android:id="@+id/imageCardanooneOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_cardano1_secondarycontainer"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_cardano1_secondarycontainer"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSenioruiuxdesig"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{jobDetailsTabContainerVM.jobDetailsTabContainerModel.txtSenioruiuxdesig}"
                        style="@style/txtPlusJakartaSansbold14_2" tools:text="@string/msg_senior_ui_ux_designer"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSmalllabelregul"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{jobDetailsTabContainerVM.jobDetailsTabContainerModel.txtSmalllabelregul}"
                        style="@style/txtPlusJakartaSansmedium12" tools:text="@string/lbl_shopee_sg"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowfulltime"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_gray_100_radius_8"
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:gravity="center"
                            android:id="@+id/btnFulltime"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxv"
                            android:layout_width="@dimen/_69pxh"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_12pxh"
                            android:paddingStart="@dimen/_12pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_fulltime"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                            android:textFontWeight="400"
                            android:textSize="@dimen/_12pxh"
                            android:textStyle="normal" tools:text="@string/lbl_fulltime"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:background="@drawable/rectangle_bg_name_gray_100_radius_8"
                            android:ellipsize="end"
                            android:fontFamily="@font/inter"
                            android:gravity="center"
                            android:id="@+id/btnTwoDaysAgo"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxv"
                            android:layout_width="@dimen/_104pxh"
                            android:letterSpacing="0.0"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_12pxh"
                            android:paddingStart="@dimen/_12pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_two_days_ago"
                            android:textAllCaps="false"
                            android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                            android:textFontWeight="400"
                            android:textSize="@dimen/_12pxh"
                            android:textStyle="normal" tools:text="@string/lbl_two_days_ago"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerListwalletOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_54pxh}" tools:itemCount="3" tools:listitem="@layout/row_one"/>
                <com.google.android.material.tabs.TabLayout
                    android:id="@+id/tabLayoutTabview"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    app:tabBackground="@drawable/selector_bg_name_gray_100_radius_22"
                    app:tabIndicatorColor="@android:color/transparent"
                    app:tabIndicatorHeight="0dp"
                    app:tabMode="scrollable"
                    app:tabSelectedTextColor="@color/{&quot;name&quot;:&quot;primary&quot;}" app:tabTextColor="@color/{&quot;name&quot;:&quot;blue_gray_300&quot;}"/>
            </LinearLayout>
            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/viewPagerTabbarview"
                android:layout_gravity="bottom|center"
                android:layout_height="match_parent"
                android:layout_weight="1" android:layout_width="match_parent"/>
        </FrameLayout>
    </LinearLayout>
</layout>
