<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onboardingThreeVM" type="com.marveloustandur.app.modules.onboardingthree.data.viewmodel.OnboardingThreeVM"/>
    </data>
    <LinearLayout android:id="@+id/linearOnboardingThree"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_29pxv"
        android:paddingEnd="@dimen/_24pxh"
        android:paddingStart="@dimen/_24pxh"
        android:paddingTop="@dimen/_29pxv" style="@style/groupStyle">
        <FrameLayout android:id="@+id/frameStack"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_699pxv" android:layout_width="@dimen/_327pxh">
            <ImageView android:id="@+id/imageImageOne"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_422pxh"
                android:layout_width="@dimen/_313pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image_422x313}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image_422x313"/>
            <FrameLayout android:id="@+id/frameStackapplicatio"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_367pxv" android:layout_width="@dimen/_327pxh">
                <com.asksira.loopingviewpager.LoopingViewPager
                    android:id="@+id/imageSliderSliderapplicati"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_367pxv"
                    android:layout_width="match_parent"
                    app:autoScroll="true" app:isInfinite="true" app:scrollInterval="5000"/>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorSliderOne"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    app:drawable_normal="@drawable/indicator_unselected_bg_name_primary_opacity_0_41_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_name_primary_radius_8"
                    app:indicator_size="@dimen/_10pxh" app:indicator_spacing="@dimen/_12pxh"/>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
