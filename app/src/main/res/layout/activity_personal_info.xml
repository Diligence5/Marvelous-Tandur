<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="personalInfoVM" type="com.marveloustandur.app.modules.personalinfo.data.viewmodel.PersonalInfoVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowbackOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowbackOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group162799"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group162799"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPersonalinfo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{personalInfoVM.personalInfoModel.txtPersonalinfo}"
                style="@style/txtPlusJakartaSansbold18" tools:text="@string/lbl_personal_info"/>
            <ImageView android:id="@+id/imageImage"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_editsquare"
                tools:ignore="ContentDescription" tools:src="@drawable/img_editsquare"/>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPersonalinfo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{personalInfoVM.personalInfoModel.txtMediumlabelsemi}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_first_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_gustavo"
                    android:id="@+id/etFirstName"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={personalInfoVM.personalInfoModel.etFirstNameValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{personalInfoVM.personalInfoModel.txtMediumlabelsemi1}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_last_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_lipshutz"
                    android:id="@+id/etLastName"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={personalInfoVM.personalInfoModel.etLastNameValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{personalInfoVM.personalInfoModel.txtMediumlabelsemi2}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_email"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_xyz_gmail_com"
                    android:id="@+id/etEmail" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={personalInfoVM.personalInfoModel.etEmailValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{personalInfoVM.personalInfoModel.txtMediumlabelsemi3}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_phone"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_1_1234567890"
                    android:id="@+id/etPhone" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={personalInfoVM.personalInfoModel.etPhoneValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{personalInfoVM.personalInfoModel.txtMediumlabelsemi4}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_location"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_lorem_ipsun"
                    android:id="@+id/etLocation"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={personalInfoVM.personalInfoModel.etLocationValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
            </LinearLayout>
        </ScrollView>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_blue_gray_50_01_radius_24"
            android:ellipsize="end"
            android:fontFamily="@font/plus_jakarta_sans"
            android:gravity="center" android:id="@+id/btnSaveChanges"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_56pxv"
            android:layout_width="@dimen/_327pxh"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@string/lbl_save_changes"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_300&quot;}"
            android:textFontWeight="600"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_save_changes"/>
    </LinearLayout>
</layout>
