<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="privacyVM" type="com.marveloustandur.app.modules.privacy.data.viewmodel.PrivacyVM"/>
    </data>
    <LinearLayout android:id="@+id/linearPrivacy"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowbackOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group162799"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group162799"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLegelandpolicie"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{privacyVM.privacyModel.txtLegelandpolicie}"
                style="@style/txtPlusJakartaSansbold18" tools:text="@string/msg_legel_and_policies"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnterms"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingEnd="@dimen/_24pxh" android:paddingStart="@dimen/_24pxh">
            <TextView android:ellipsize="end" android:id="@+id/txtTerms"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{privacyVM.privacyModel.txtTerms}"
                style="@style/txtPlusJakartaSansbold16" tools:text="@string/lbl_terms"/>
            <TextView android:alpha="0.5"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{privacyVM.privacyModel.txtDescription}"
                style="@style/txtPlusJakartaSansmedium14_4" tools:text="@string/msg_lorem_ipsum_dolor9"/>
            <TextView android:alpha="0.5"
                android:id="@+id/txtDescription1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{privacyVM.privacyModel.txtDescription1}"
                style="@style/txtPlusJakartaSansmedium14_4" tools:text="@string/msg_lorem_ipsum_dolor9"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtChangestotheser"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{privacyVM.privacyModel.txtChangestotheser}"
                style="@style/txtPlusJakartaSansbold16" tools:text="@string/msg_changes_to_the_service"/>
            <FrameLayout android:id="@+id/frameStackdescriptio"
                android:layout_height="@dimen/_298pxv" android:layout_width="@dimen/_307pxh">
                <TextView android:alpha="0.5"
                    android:gravity="top|start"
                    android:id="@+id/txtDescription2"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@{privacyVM.privacyModel.txtDescription2}"
                    style="@style/txtPlusJakartaSansmedium14_4" tools:text="@string/msg_lorem_ipsum_dolor9"/>
                <TextView android:alpha="0.5"
                    android:gravity="top|start"
                    android:id="@+id/txtDescription3"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@{privacyVM.privacyModel.txtDescription3}"
                    style="@style/txtPlusJakartaSansmedium14_4" tools:text="@string/msg_lorem_ipsum_dolor9"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
