<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="applyJobVM" type="com.marveloustandur.app.modules.applyjob.data.viewmodel.ApplyJobVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowbackOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700_01&quot;}">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowbackOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group162799"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group162799"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtApplyjob"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{applyJobVM.applyJobModel.txtApplyjob}"
                style="@style/txtPlusJakartaSansbold18" tools:text="@string/lbl_apply_job"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearApplyjob"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_31pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_31pxv">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInputfield"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{applyJobVM.applyJobModel.txtMediumlabelsemi}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_full_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/msg_brooklyn_simmons"
                    android:id="@+id/etFullName"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={applyJobVM.applyJobModel.etFullNameValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInputfield1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi1"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{applyJobVM.applyJobModel.txtMediumlabelsemi1}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_email_address"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_xyz_gmail_com"
                    android:id="@+id/etEmail" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={applyJobVM.applyJobModel.etEmailValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMediumlabelsemi2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{applyJobVM.applyJobModel.txtMediumlabelsemi2}"
                style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/lbl_upload_cv"/>
            <LinearLayout android:id="@+id/linearColumnclouduplo"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_39pxv"
                android:paddingEnd="@dimen/_125pxh"
                android:paddingStart="@dimen/_125pxh"
                android:paddingTop="@dimen/_39pxv" style="@style/groupStylenameonPrimaryContaineropacity1cornerRadius_9">
                <ImageView android:id="@+id/imageClouduploadone"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_cloudupload1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_cloudupload1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUploadfile"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{applyJobVM.applyJobModel.txtUploadfile}"
                    style="@style/txtPlusJakartaSanssemibold14" tools:text="@string/lbl_upload_file"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInputfield2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMediumlabelsemi3"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{applyJobVM.applyJobModel.txtMediumlabelsemi3}"
                    style="@style/txtPlusJakartaSansmedium14_1" tools:text="@string/msg_website_blog_or"/>
                <EditText
                    android:background="@drawable/rectangle_bg_name_onprimarycontainer_opacity_1_border_name_indigo_50_radius_24"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:hint="@string/lbl_https"
                    android:id="@+id/etFrameOne"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={applyJobVM.applyJobModel.etFrameOneValue}"
                    android:textColor="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textColorHint="@color/{&quot;name&quot;:&quot;blue_gray_400&quot;}"
                    android:textFontWeight="500"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_24"
            android:ellipsize="end"
            android:fontFamily="@font/plus_jakarta_sans"
            android:gravity="center" android:id="@+id/btnContinue"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_56pxv"
            android:layout_width="@dimen/_327pxh"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@string/lbl_continue"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;gray_50_01&quot;}"
            android:textFontWeight="600"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_continue"/>
    </LinearLayout>
</layout>
