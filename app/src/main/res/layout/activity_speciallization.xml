<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="speciallizationVM" type="com.marveloustandur.app.modules.speciallization.data.viewmodel.SpeciallizationVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnwhatisyourspeci"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle{&quot;name&quot;:&quot;white_A700_01&quot;}">
        <LinearLayout android:id="@+id/linearSpeciallization"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_23pxh"
            android:paddingStart="@dimen/_23pxh" android:paddingTop="@dimen/_13pxv">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="start"
                android:layout_height="@dimen/_24pxh"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group162799"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group162799"/>
            <TextView android:id="@+id/txtWhatisyourspeci"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{speciallizationVM.speciallizationModel.txtWhatisyourspeci}"
                style="@style/txtPlusJakartaSansbold24" tools:text="@string/msg_what_is_your_specialization"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtLoremipsumdolor"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{speciallizationVM.speciallizationModel.txtLoremipsumdolor}"
                style="@style/txtPlusJakartaSansmedium14" tools:text="@string/msg_lorem_ipsum_dolor2"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumndesigncre"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_10pxv" style="@style/groupStylenameonPrimaryContaineropacity1cornerRadius_4">
                <androidx.appcompat.widget.AppCompatCheckBox
                    android:drawablePadding="@dimen/_16pxh"
                    android:drawableStart="@drawable/drawable_img_checkmark_primary"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/checkBoxDesigncreative"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@string/msg_design_creative"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/msg_design_creative"/>
            </LinearLayout>
            <RadioGroup android:gravity="center_horizontal"
                android:id="@+id/rgGroup179"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <RadioButton android:drawablePadding="@dimen/_17pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbDevelopmentit"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/msg_development_it"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/msg_development_it"/>
                <RadioButton android:drawablePadding="@dimen/_17pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbEngineeringarch"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/msg_engineering_architecture"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/msg_engineering_architecture"/>
                <RadioButton android:drawablePadding="@dimen/_17pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbSalesmarketing"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/msg_sales_marketing"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/msg_sales_marketing"/>
            </RadioGroup>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnwriting"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_10pxv" style="@style/groupStylenameonPrimaryContaineropacity1cornerRadius_4">
                <RadioButton android:drawablePadding="@dimen/_17pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbWriting"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_writing"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_writing"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnfinance"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_12pxv" style="@style/groupStylenameonPrimaryContaineropacity1cornerRadius_4">
                <RadioButton android:drawablePadding="@dimen/_17pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbFinance"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingTop="@dimen/_1pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_finance"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_finance"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:background="@drawable/rectangle_bg_name_primary_radius_24"
            android:ellipsize="end"
            android:fontFamily="@font/plus_jakarta_sans"
            android:gravity="center" android:id="@+id/btnContinue"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_56pxv"
            android:layout_width="@dimen/_327pxh"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@string/lbl_continue"
            android:textAllCaps="false"
            android:textColor="@color/{&quot;name&quot;:&quot;gray_50_01&quot;}"
            android:textFontWeight="600"
            android:textSize="@dimen/_16pxh" android:textStyle="bold" tools:text="@string/lbl_continue"/>
    </LinearLayout>
</layout>
