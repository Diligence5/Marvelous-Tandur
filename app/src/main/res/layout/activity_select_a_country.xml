                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbAngola"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_angola"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_angola"/>
                <RadioButton android:drawablePadding="@dimen/_16pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbAntiguaandbarbu"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@string/msg_antigua_and_barbuda"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/msg_antigua_and_barbuda"/>
                <RadioButton android:drawablePadding="@dimen/_16pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbArgentina"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_argentina"
                    android:textColor="@color/{&quot;name&quot;:&quot;primary&quot;}"
                    android:textFontWeight="600"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/lbl_argentina"/>
                <RadioButton android:drawablePadding="@dimen/_16pxh"
                    android:drawableStart="@drawable/img_settings"
                    android:ellipsize="end"
                    android:fontFamily="@font/plus_jakarta_sans"
                    android:id="@+id/rbArgentinaOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
